                                                <span>Casa dos Répteis</span>
                                            </div>
                                            <div class="col-sm-1">
                                                <i data-feather="edit"></i>
                                            </div>
                                            <div class="col-sm-1">
                                                <i data-feather="trash-2"></i>
                                            </div>
                                            <div class="col-sm-1">
                                                <i data-feather="eye"></i>
                                            </div>
                                        </div>                                        
                                    </div>
                                </div>  

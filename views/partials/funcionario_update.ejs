<div class="form-group">
    <input type="hidden" name="CPF">
    <label for="CPF">CPF</label>
    <input type="number" id="CPF" name="CPF" value= <%= CPF %>>
</div>
<div class="form-group">
    <label for="nome">Nome</label>
    <input type="text" id="nome" name="nome" value= <%= nome %> >
</div>
<div class="form-group">
    <label for="ddn">Data de Nascimento</label>
    <input type="date" id="ddn" name="ddn" value= <%= ddn %> readonly>
</div>
<div class="form-group">
    <label for="salario">Salário</label>
    <input type="number" id="salario" name="salario" value= <%= salario %>>
</div>
<div class="form-group">
    <label for="CLT">CLT</label>
    <input type="number" id="CLT" name="CLT" value= <%= CLT %>>
</div>
<div class="form-group">
    <label for="endereco">Endereço</label>
    <input type="text" id="endereco" name="endereco" value= <%= endereco %>>
</div>
<div class="form-group">
    <label for="banco">Banco</label>
    <input type="number" id="banco" name="banco" value= <%= banco %>>
</div>
<div class="form-group">
    <label for="agencia">Agência</label>
    <input type="number" id="agencia" name="agencia" value= <%= agencia %>>
</div>
<div class="form-group">
    <label for="conta">Conta</label>
    <input type="number" id="conta" name="conta" value= <%= conta %>>
</div>
<div class="form-group">
    <label for="digito">Dígito</label>
    <input type="number" id="digito" name="digito" value= <%= digito %>>
</div>